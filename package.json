{
  "name": "simple-stock-screener",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun biome ci && bun typecheck && bun react-router build",
    "dev": "react-router dev",
    "wrangler:dev": "wrangler dev",
    "update": "bunx npm-check-updates -ui --reject bun",
    "wrangler:typegen": "wrangler types",
    "db:studio": "drizzle-kit studio --config ./src/shared/config/drizzle-kit.ts",
    "db:generate": "drizzle-kit generate --config ./src/shared/config/drizzle-kit.ts",
    "db:push": "drizzle-kit push --config ./src/shared/config/drizzle-kit.ts",
    "db:migrate": "wrangler d1 migrations apply simple-stock-screener --remote",
    "inngest:dev": "inngest-cli dev --no-discovery -u http://localhost:5173/api/inngest",
    "inngest:sync": "curl -X PUT https://simple-stock-screener.ctoofeverything.dev/api/inngest",
    "typecheck": "react-router typegen && tsc -b",
    "check": "biome check --write",
    "deploy": "bun wrangler deploy && bun inngest:sync"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.1.9",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-dropdown-menu": "^2.1.5",
    "@radix-ui/react-popover": "^1.1.5",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@react-router/fs-routes": "^7.1.5",
    "@react-router/node": "^7.1.5",
    "@react-router/serve": "^7.1.5",
    "@tabler/icons-react": "^3.29.0",
    "@tanstack/react-table": "^8.20.6",
    "ai": "^4.1.17",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.39.1",
    "inngest": "^3.31.3",
    "isbot": "^5.1.22",
    "marked": "^15.0.6",
    "motion": "^12.0.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.0.3",
    "react-router": "^7.1.5",
    "react-use-measure": "^2.1.7",
    "tailwind-merge": "^3.0.1",
    "tailwind-variants": "^0.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@cloudflare/workers-types": "^4.20250129.0",
    "@cto_of_everything/biome-config": "^1.1.3",
    "@react-router/dev": "^7.1.5",
    "@tailwindcss/vite": "^4.0.3",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^22",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.20",
    "better-sqlite3": "^11.8.1",
    "drizzle-kit": "^0.30.4",
    "inngest-cli": "^1.4.5",
    "postcss": "^8.5.1",
    "tailwindcss": "^4.0.3",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^3.107.2"
  },
  "trustedDependencies": [
    "inngest-cli",
    "better-sqlite3"
  ]
}